mybatis-plus:
  mapper-locations: classpath*:/${xmlPath}/*.xml
  typeAliasesPackage: ${entityPackage}
  # 测试环境打印sql
  configuration:
    log-impl: org.apache.ibatis.logging.stdout.StdOutImpl

