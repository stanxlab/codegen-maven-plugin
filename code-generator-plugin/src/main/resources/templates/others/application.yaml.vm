server:
  port: 8090

spring:
  profiles:
    active: local
  application:
    name: ${baseProjectName}
  mvc:
    pathmatch:
      matching-strategy: ant_path_matcher
  datasource:
    url: ${dbInfo.url}
    username: ${dbInfo.username}
    password: ${dbInfo.password}

#if($ormName == 'mybatis-plus')
